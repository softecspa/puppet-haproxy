
backend <%= backend_name %>
    mode <%= mode %>
<% scope.lookupvar('array_options').each do |option| -%>
<% if option != '' -%>
    option <%= option %>
<% end -%>
<% end -%>
<% if @timeout_connect!='' -%>    timeout connect <%= @timeout_connect -%><% end %>
<% if @timeout_client!='' -%>    timeout client <%= @timeout_client -%><% end %>
<% if @timeout_server!='' -%>    timeout server <%= @timeout_server -%><% end %>
